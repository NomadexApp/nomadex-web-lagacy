{
	"name": "voi.nomadex.app",
	"version": "0.0.1",
	"browser": {
		"crypto": false,
		"path": false,
		"fs": false
	},
	"devDependencies": {
		"@algorandfoundation/algokit-client-generator": "^2.6.0",
		"@algorandfoundation/algokit-utils": "^5.3.2",
		"@algorandfoundation/tealscript": "0.80.1",
		"@blockshake/defly-connect": "^1.1.6",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-cloudflare": "^3.0.1",
		"@sveltejs/kit": "^1.5.0",
		"@tailwindcss/typography": "^0.5.10",
		"@txnlab/use-wallet": "^2.5.0",
		"@types/luxon": "^3.3.7",
		"@types/node": "^20.10.5",
		"@types/ungap__structured-clone": "^1.2.0",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"@ungap/structured-clone": "^1.2.0",
		"@walletconnect/client": "^1.8.0",
		"@walletconnect/types": "^2.11.0",
		"algosdk": "^2.7.0",
		"arc200js": "^2.3.2",
		"autoprefixer": "^10.4.16",
		"buffer": "^6.0.3",
		"daisyui": "^4.4.23",
		"eslint": "^8.56.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-svelte": "^2.35.1",
		"js-sha512": "^0.8.0",
		"lightweight-charts": "^4.1.1",
		"postcss": "^8.4.32",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.69.7",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-lightweight-charts": "^2.2.0",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"url": "^0.11.3",
		"vite": "^4.3.0"
	},
	"private": true,
	"scripts": {
		"dev": "vite dev --port 5210",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint --ignore-pattern src/contracts .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate-client": "for K in src/contracts/artifacts/*.arc32.json; do X=\"${K/artifacts/clients}\"; J=\"${X/.arc32.json/Client.ts}\"; algokitgen generate -a $K -o $J; done",
		"compile-contract": "tealscript src/contracts/*.algo.ts src/contracts/artifacts",
		"compile": "npm run compile-contract && npm run generate-client",
		"fix": "eslint . --ext .ts --fix"
	},
	"type": "module",
	"dependencies": {
		"chart.js": "^4.4.1",
		"chartjs-adapter-luxon": "^1.3.1",
		"chartjs-chart-financial": "^0.1.1",
		"firebase": "^10.7.1",
		"luxon": "^3.4.4",
		"svelte-icons": "^2.1.0",
		"svelte-star": "https://github.com/sveltedeveth/svelte-star.git"
	}
}